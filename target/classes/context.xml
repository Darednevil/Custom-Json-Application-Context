<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="io.zensoft.context.service.xml.DefaultVodkaService" id="vodkaService">
        <constructor-arg name="vodka" value="Vodka"/>
    </bean>

    <bean class="io.zensoft.context.service.xml.DefaultScotchService" id="scotchService">
        <constructor-arg name="scotch" value="Scotch"/>
    </bean>

</beans>